      ],
      "settings": {
        "foreground": "#846905"
      }
    },
    {
      "name": "gold100",
      "scope": ["constant", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#413900"
      }
    },
    {
      "name": "gold500",
      "scope": [
        "keyword.operator",
        "keyword.control",
        "storage",
        "variable.language"
      ],
      "settings": {
        "foreground": "#CE9409"
      }
    },
    {
      "name": "sea300",
      "scope": ["entity.name.function", "source.python"],
